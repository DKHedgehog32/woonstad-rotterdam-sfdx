<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MISSING_OUTCOME</fullName>
    <active>true</active>
    <description>Outcome is a required field when the value in the Indicator Assignment Type field is Outcome.</description>
    <errorConditionFormula>ISPICKVAL(  IndicatorAssignmentType ,&quot;Outcome&quot;) &amp;&amp; ISBLANK(  OutcomeId  )</errorConditionFormula>
    <errorDisplayField>OutcomeId</errorDisplayField>
    <errorMessage>Complete this field.</errorMessage>
</ValidationRule>
