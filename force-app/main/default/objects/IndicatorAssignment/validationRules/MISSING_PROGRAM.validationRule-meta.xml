<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MISSING_PROGRAM</fullName>
    <active>true</active>
    <description>Program is a required field when the value in the Indicator Assignment Type field is Program.</description>
    <errorConditionFormula>ISPICKVAL(  IndicatorAssignmentType ,&quot;Program&quot;) &amp;&amp; ISBLANK(  ProgramId  )</errorConditionFormula>
    <errorDisplayField>ProgramId</errorDisplayField>
    <errorMessage>Complete this field.</errorMessage>
</ValidationRule>
