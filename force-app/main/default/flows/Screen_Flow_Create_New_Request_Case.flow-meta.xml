    <formulas>
        <name>forT_DefaultCaseType</name>
        <dataType>String</dataType>
        <expression>IF(
    CONTAINS(LOWER({!Subject}), &quot;naamplaat&quot;),
    &quot;Name Plate&quot;,
    IF(
        CONTAINS(LOWER({!Subject}), &quot;reparatie&quot;),
        &quot;Repair Request&quot;,
        IF(
            OR(
                 CONTAINS(LOWER({!Subject}), &quot;onderhoud&quot;),
                 CONTAINS(LOWER({!Subject}), &quot;inspectie&quot;)
                 ),
            &quot;Maintenance Inspection&quot;,
            IF(
                CONTAINS(LOWER({!Subject}), &quot;verklaring&quot;),
                &quot;Landlord Statement&quot;,
                 IF(
                     OR(
                          CONTAINS(LOWER({!Subject}), &quot;voorziening&quot;),
                          CONTAINS(LOWER({!Subject}), &quot;aangebracht&quot;)
                          ),
                    &quot;Self-Installed Facility&quot;,
                     IF(
                         CONTAINS(LOWER({!Subject}), &quot;betaalregeling&quot;),
                         &quot;Payment Arrangement&quot;,
                &quot;&quot;
       )))))
)</expression>
    </formulas>
    <formulas>
        <name>forT_DefaultPriority</name>
        <dataType>String</dataType>
        <expression>IF(
   OR(
      CONTAINS(LOWER({!Subject}),&quot;urgent&quot;),
      CONTAINS(LOWER({!Subject}),&quot;belang&quot;)
        ),
        &quot;High&quot;, &quot;Medium&quot;
    )</expression>
    </formulas>
