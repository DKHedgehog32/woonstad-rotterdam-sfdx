        </defaultConnector>
        <defaultConnectorLabel>Other</defaultConnectorLabel>
        <rules>
            <name>Tenant</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Role__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>HUU</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Active__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Tenant_Value</targetReference>
            </connector>
            <label>Tenant</label>
        </rules>
        <rules>
            <name>Former_Tenant</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Role__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>HUU</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Role__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MDH</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Active__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Former_Tenant_Value</targetReference>
            </connector>
            <label>Former Tenant</label>
        </rules>
        <rules>
            <name>Owner</name>
            <conditionLogic>(1 OR 2) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Role__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>EIG</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Role__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>KOP</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Active__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Owner_Value</targetReference>
            </connector>
            <label>Owner</label>
        </rules>
        <rules>
            <name>Co_Tenant</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Role__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>MDH</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Active__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Co_Tenant_Value</targetReference>
            </connector>
            <label>Co-Tenant</label>
        </rules>
        <rules>
            <name>Payer</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Role__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>BET</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Search_Through_Contract_Contact_Roles.Active__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Payer_Value</targetReference>
            </connector>
            <label>Payer</label>
        </rules>
    </decisions>
